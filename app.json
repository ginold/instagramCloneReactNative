{
  "expo": {
    "name": "Instagram Clone",
    "slug": "instagramclone",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "owner": "ginold",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.ginold.instaclone",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.ginold.instaclone",
      "versionCode": 1
    },
    "description": "Instagram React Native Clone",
    "githubUrl": "https://github.com/ginold/instagramCloneReactNative",
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#000000"
    }
  }
}
